<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:ads="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="mert.pokemongoelkitabi.mert.pokemongoelkitabi.Terimler"
    tools:showIn="@layout/app_bar_terimler"
    android:background="@drawable/side_nav_bar"
    >

<ScrollView
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Achievement(Başarı)"
            android:id="@+id/textView16"
            android:textColor="#660033"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Oyunda yerine getirilen görevler sonucunda rozet alırsınız. Bu rozetler profilinizde görünür. "
            android:id="@+id/textView17" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="Charged move"
            android:textColor="#660033"
            android:id="@+id/textView18" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemonların yaptığı güçlü saldırı çeşididir. Savaşta yeteri kadar stamina(gücünüz) var ise ekrana basılı tuttuğunuzda aktifleşen saldırı çeşididir. "
            android:id="@+id/textView19" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="CP(Combat Point)"
            android:id="@+id/textView20"
            android:textColor="#660033"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemonun savunma ve saldırı özelliklerini gösteren savaş puanıdır. Pokemonun gücünü gösterir. "
            android:id="@+id/textView21" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:text="HP"
            android:textColor="#660033"
            android:id="@+id/textView22" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemonun yenilmesi için alması gereken hasar miktarıdır."
            android:id="@+id/textView23" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Dodge"
            android:id="@+id/textView24" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Savaş esnasında saldırıdan hasar almadan kurtulmayı ifade eder. "
            android:id="@+id/textView25" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Evolution"
            android:id="@+id/textView26" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemonun evrimleşmesidir. evrimleşen pokemon’un CP’si ve istatistikleri artar."
            android:id="@+id/textView27" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Experience Point(xp)"
            android:id="@+id/textView28" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Deneyim puanlarıdır. Yaptığınız aktiviteler sonucu pokemon yakalamak vs) artar ve seviye atlamanızı sağlar. "
            android:id="@+id/textView29" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Pokeball"
            android:id="@+id/textView30" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemon yakalayabilmek için ihtiyacınız olan eşyalardır. Eğer Poke toplarınız biterse Pokemon yakalayamazsınız."
            android:id="@+id/textView31" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Poke Stop"
            android:id="@+id/textView32" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Etrafınızdaki yerlerde genellikler merkezi yerlerde (heykel, müze, meydan, camii gibi) oyunu haritasında mavi daire şeklinde görünen duraklardır. Bu duraklar ücretsiz Poke topları, Pokemon yumurtaları ve diğer eşyalar verir. "
            android:id="@+id/textView33" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Poke Coin"
            android:id="@+id/textView34" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemon para birimidir."
            android:id="@+id/textView35" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Pokemon Gym"
            android:id="@+id/textView36" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    5. Seviyeye ulaşıldığında gerçek oyuncularla mücadele etmek için gidilen arenalardır."
            android:id="@+id/textView37" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Pokemon Yumurtaları"
            android:id="@+id/textView38" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Poke Stoplarda bulunan ve içinden pokemon çıkan yumurtalardır. Yumurtalar alındıktan sonra çatlamaları için egg incubatorlara konulup belirli bir mesafe yürümek gerekir."
            android:id="@+id/textView39" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Pokedex"
            android:id="@+id/textView40" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Yakaladığınız, karşılaştığınız pokemonlara ve pokemonların özelliklerine ulaşabileceğiniz yerdir."
            android:id="@+id/textView41" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Pokemon Candy"
            android:id="@+id/textView42" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemon yakaladıkça ya da transfer ettikçe kazanılır. Her Pokemonun, kendi ismine özel Candysi vardır. Pokemonları güçlendirmek ve evrimleştirmek (evolve) için kullanılır."
            android:id="@+id/textView43" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Stardust"
            android:id="@+id/textView44" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemon yakaladıkça ya da transfer ettikçe kazanırsınız. Stardust tüm pokemonlar için ortaktır."
            android:id="@+id/textView45" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Potion"
            android:id="@+id/textView46" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Gymlerde savaşan ve yaralanan Pokemonları iyileştirir. 3 türü vardır: Normal Potion 20 HP iyileştirir, Super Potion 50 HP iyileştirir, Hyper Potion 200 HP iyileştirir."
            android:id="@+id/textView47" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Revive"
            android:id="@+id/textView48" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Savaşlarda rakip takım tarafından baygın düşürülmüş Pokemonları canlandırır. Poke Stoplarda bulunur."
            android:id="@+id/textView49" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Incense"
            android:id="@+id/textView50" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Etrafta Pokemon bulma olasılığınızı arttırır. PokeCoinlerle satın alınabilir."
            android:id="@+id/textView51" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Razz Berry"
            android:id="@+id/textView52" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="#660033"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Zor yakalanan bir pokemonu yakalama şansını arttıran meyvelerdir. "
            android:id="@+id/textView53" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="#660033"
            android:text="Egg Incubator"
            android:id="@+id/textView54" />

        <TextView
            android:layout_width="wrap_content"
            android:textColor="#660033"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceSmall"
            android:text="    Pokemon yumurtalarını koyup, siz yürüdükçe onların kırılmaya hazır hale gelmesini sağlayan eşyalardır. Aynı anda sadece 1 yumurta üzerinde kullanabilir, sınırsız sayıda kullanabilirsiniz. Seviye atladıkça 3 kullanımlık Egg Incubatorlar ile aynı anda birkaç yumurta açabilirsiniz. Oyuna 1 taneyle başlanır.\n\n\n\n"
            android:id="@+id/textView55" />
    </LinearLayout>

</ScrollView>


    <com.google.android.gms.ads.AdView android:id="@+id/adView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        ads:adUnitId="ca-app-pub-5677978125129640/7501373011"
        ads:adSize="BANNER"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

</RelativeLayout>
